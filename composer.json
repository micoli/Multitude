{
    "name": "micoli/multitude",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "loophp/collection": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "vimeo/psalm": "^5.9",
        "friendsofphp/php-cs-fixer": "^3.16",
        "phpdocumentor/reflection-docblock": "^5.3",
        "symfony/var-dumper": "^6.2",
        "php-coveralls/php-coveralls": "^0.1.0",
        "phpstan/phpstan": "^1.10",
        "phan/phan": "^5.4",
        "ramsey/uuid": "^4.7"
    },
    "autoload": {
        "psr-4": {
            "Micoli\\Multitude\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Micoli\\Multitude\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Olivier MICHAUD",
            "email": "olivier@micoli.org"
        }
